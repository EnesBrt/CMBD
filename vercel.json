{
    "builds": [
        {
            "src": "build.sh",
            "use": "@vercel/static-build",
            "config": { "distDir": "CMDB" }
        },
        {
            "src": "CMDB/wsgi.py",
            "use": "@vercel/python",
            "config": { "maxLambdaSize": "15mb", "runtime": "python3.9.12" }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "CMDB/wsgi.py"
        }
    ]
}